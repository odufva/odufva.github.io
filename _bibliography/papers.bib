	author = {Schubert, Claudia and Chatain, Nicolas and Braunschweig, Till and Schemionek, Mirle and Feldberg, Kristina and Hoffmann, Melanie and Dufva, Olli and Mustjoki, Satu and Brümmendorf, Tim H. and Koschmieder, Steffen},
